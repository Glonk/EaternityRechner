<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
	xmlns:g='urn:import:com.google.gwt.user.client.ui'
	xmlns:rechner='urn:import:ch.eaternity.client'>


	<ui:style>
		.panel {
			padding: 10px;
		}
		
		.aboutText {
			text-align: left;
		}
		
		.buttons {
			text-align: right;
		}
	</ui:style>
	
	<g:HTMLPanel>
		<g:HTMLPanel width='640px' styleName='{style.panel}'
			ui:field='previewMenuHtmlPanel'>
		</g:HTMLPanel>
		<div class='{style.buttons}'>
			<g:Button text='Einverstanden. Gehe zum Rechner.' ui:field='closeButton' />
		</div>
	</g:HTMLPanel>
</ui:UiBinder>
